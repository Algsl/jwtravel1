.main{
  width: 100%;
  height: 100%;
  position: fixed;
  background: #F7F9FC;
}
.top{
  width: 100%;
  height:100rpx;
  display: flex;
  flex-direction: row;
  background:#ffffff;
  text-align: center;
}

.top_item{
  width: 25%;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 20rpx;
  text-align: center;
  color: #A0A3AF;
  font-size: 28rpx;
}

.travel{
  width: 100%;
  height: 485rpx;
  display: flex;
  flex-direction: row;
}

.travel_item{
  width: 340rpx;
  height: 470rpx;
  margin-top: 15rpx;
  margin-left: 25rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
}
.travel_item_img{
  width: 340rpx;
  height: 350rpx;
}
.travel_item_title{
  margin-left: 20rpx;
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #434343;
}
.travel_item_bottom{
  width: 340rpx;
  height: 40rpx;
  margin-left: 20rpx;
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
}
.travel_item_price{
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #FF4081;
}
.travel_item_selld{
  width: 150rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-left: 70rpx;
  font-size: 28rpx;
  color: #A0A3AF;
}


.on{
  color: #1AAE82;
  border-bottom: 5rpx solid #1AAE82;
}
::-webkit-scrollbar{
  width: 0rpx;
  height: 0rpx;
  color: transparent;
}